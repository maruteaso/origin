{
  "name": "youtube-downloader",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=12.16.0"
  },
  "scripts": {
    "_サーバー関係": "",
    "server": "nodemon --exec cross-env TS_NODE_PROJECT=tsconfig-for-node.json ts-node ./server/server.ts --watch server",
