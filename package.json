    "_ローカルでのテスト": "",
    "download": "cross-env TS_NODE_PROJECT=tsconfig-for-node.json ts-node scripts/download.ts",
    "_heroku用のコマンド": "",
    "heroku-postbuild": "yarn server:build"
  },
