    "@types/webpack": "^5.28.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^2.5.2"
  }
